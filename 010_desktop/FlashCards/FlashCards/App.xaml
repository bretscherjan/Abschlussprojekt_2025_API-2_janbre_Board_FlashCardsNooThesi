<Application x:Class="FlashCards.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FlashCards"
             xmlns:conv="clr-namespace:FlashCards.Converters"
             StartupUri="Index.xaml">
    <Application.Resources>
        <conv:BoolToResultTextConverter x:Key="BoolToResultTextConverter"/>
        <conv:ResultToColorConverter x:Key="ResultToColorConverter"/>
    </Application.Resources>
</Application>